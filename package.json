        "dev:create": "bin/dm doc -t ./example/api",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/zhouzuchuan/data-mock.git"
    },
